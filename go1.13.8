+// 0101 xxxm mmnn nttt
+func _MOVW__Rn_Rm__Rt(c *Ctx, p *obj.Prog, out []uint16) int {
+	var o1 int
+	mem, r := &p.From, &p.To
+	if r.Type == obj.TYPE_REG {
+		switch p.As {
+		case AMOVW: // LDR
+			o1 = 0x5800
+		case AMOVB: // LDRSB
+			o1 = 0x5600
+		case AMOVBU: // LDRB
+			o1 = 0x5C00
+		case AMOVH: // LDRSH
+			o1 = 0x5E00
+		default: // AMOVHU (LDRH)
+			o1 = 0x5A00
+		}
+	} else {
+		mem, r = r, mem
+		switch p.As {
+		case AMOVW: // STR
+			o1 = 0x5000
+		case AMOVB, AMOVBU: // STRB
+			o1 = 0x5400
+		default: // AMOVH, AMOVHU (STRH)
+			o1 = 0x5200
+		}
+	}
+	Rn := int(mem.Reg)
+	Rm := int(mem.Index)
+	Rt := int(r.Reg)
+	out[0] = uint16(o1 | Rm&7<<6 | Rn&7<<3 | Rt&7)
+	return 2
+}
