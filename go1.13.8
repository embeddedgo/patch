+// 1111 100x 0xxx nnnn  tttt 0000 00uu mmmm
+func _MOVW__Rn_Rm_1_u2__Rt(c *Ctx, p *obj.Prog, out []uint16) int {
+	o1, o2, a := oldrstr32(p)
+	var u2 int
+	switch a.Scale {
+	case 0, 1:
+		break
+	case 2:
+		u2 = 1
+	case 4:
+		u2 = 2
+	case 8:
+		u2 = 3
+	default:
+		log.Fatal("bad scale")
+	}
+	Rm := int(a.Index)
+	out[0] = uint16(o1)
+	out[1] = uint16(o2 | u2<<4 | Rm&15)
+	return 4
+}
