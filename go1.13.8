+func _MOVW__Rm__Rd(c *Ctx, p *obj.Prog, out []uint16) int {
+	Rm := int(p.From.Reg)
+	Rd := int(p.To.Reg)
+	if p.Scond&C_SBIT == 0 {
+		out[0] = uint16(0x4600 | Rd&8<<4 | Rm&15<<3 | Rd&7)
+	} else {
+		out[0] = uint16(0x0000 | Rm&7<<3 | Rd&7)
+	}
+	return 2
+}
