+// 1111 1010 10xx nnnn  1111 dddd 10xx mmmm
+func _CLZ__Rm__Rd(c *Ctx, p *obj.Prog, out []uint16) int {
+	o1, o2 := 0xFA90, 0xF080 // AREV
+	Rm := int(p.From.Reg)
+	Rn := Rm
+	Rd := int(p.To.Reg)
+	switch p.As {
+	case ACLZ:
+		o1 += 0x0020
+	case AREV16:
+		o2 |= 0x0010
+	case ARBIT:
+		o2 |= 0x0020
+	case AREVSH:
+		o2 |= 0x0030
+	case ASEL:
+		o1 += 0x0010
+		if p.Reg != 0 {
+			Rn = int(p.Reg)
+		} else {
+			Rn = Rd
+		}
+	}
+	out[0] = uint16(o1 | Rn&15)
+	out[1] = uint16(o2 | Rd&15<<8 | Rm&15)
+	return 4
+}
