+// 1100 xnnn rrrr rrrr
+func _MOVM_IAW(c *Ctx, p *obj.Prog, out []uint16) int {
+	o1 := 0xC000
+	rlist, Rn := int(p.From.Offset), int(p.To.Reg)
+	if p.To.Type == obj.TYPE_REGLIST {
+		if !c.checkldm(p) {
+			return 0
+		}
+		rlist, Rn = int(p.To.Offset), int(p.From.Reg)
+		o1 |= 0x0800
+	}
+	out[0] = uint16(o1 | Rn&15<<8 | rlist)
+	return 2
+}
