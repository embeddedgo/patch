+// 1111 0jii iiii iiii  1xj1 jiii iiii iiii
+func _B__ji24_1(c *Ctx, p *obj.Prog, out []uint16) int {
+	o1 := 0xF000
+	o2 := 0x9000
+	if p.As == ABL {
+		o2 |= 0x4000
+	}
+	v := c.boffsetrel(p, o1, o2)
+	s := v >> 23 & 1
+	j1 := ^(v>>22 ^ s) & 1
+	j2 := ^(v>>21 ^ s) & 1
+	imm10 := v >> 11 & 0x3FF
+	imm11 := v & 0x7FF
+	out[0] = uint16(o1 | s<<10 | imm10)
+	out[1] = uint16(o2 | j1<<13 | j2<<11 | imm11)
+	return 4
+}
