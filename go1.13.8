+// 1111 0u10 x0x0 nnnn  0uuu dddd uuuu uuuu
+func _ADD__u12__Rn__Rd(c *Ctx, p *obj.Prog, out []uint16) int {
+	imm := int(p.From.Offset)
+	Rd := int(p.To.Reg) & 15
+	Rn := Rd
+	if p.Reg != 0 {
+		Rn = int(p.Reg) & 15
+	}
+	o1 := imm>>1&0x400 | Rn
+	o2 := imm&0x700<<4 | Rd<<8 | imm&0xFF
+	switch p.As {
+	case AADD:
+		o1 |= 0xF200
+	default: // ASUB
+		o1 |= 0xF2A0
+	}
+	out[0] = uint16(o1)
+	out[1] = uint16(o2)
+	return 4
+}
