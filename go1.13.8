+	MOVW  $SYS_getpid, R7
+	SWI   $0
+	MOVW  R0, R4
+	MOVW  $SYS_gettid, R7
+	SWI   $0
+	MOVW  R0, R1         // arg 2 tid
+	MOVW  R4, R0         // arg 1 pid
+	MOVW  sig+0(FP), R2  // arg 3
+	MOVW  $SYS_tgkill, R7
+	SWI   $0
+	RET   
+
