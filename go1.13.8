+func lookupFuncSym(syms *sym.Symbols, name string) *sym.Symbol {
+	if s := syms.ROLookup(name, sym.SymVerABI0); s != nil && s.FuncInfo != nil {
+		return s
+	}
+	if s := syms.ROLookup(name, sym.SymVerABIInternal); s != nil && s.FuncInfo != nil {
+		return s
+	}
+	return nil
+}
