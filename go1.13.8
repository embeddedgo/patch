+const (
+	CL1I  CLIDR = 1 << 0    //+ Instruction L1 cache implemented.
+	CL1D  CLIDR = 1 << 1    //+ Data cache L1 implemented.
+	CL1U  CLIDR = 1 << 2    //+ Unified L1 cache.
+	CL2I  CLIDR = 1 << 3    //+ Instruction L2 cache implemented.
