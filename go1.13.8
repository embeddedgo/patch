+TEXT runtime·open(SB),NOSPLIT,$0
+	MOVW     name+0(FP), R0
+	MOVW     mode+4(FP), R1
+	MOVW     perm+8(FP), R2
+	MOVW     $SYS_open, R7
+	SWI      $0
+	MOVW     $0xfffff001, R1
