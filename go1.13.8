+		o1 |= 0x0010
+	case ASTREXH:
+		o2 |= 0x0010
+	case ALDREXH:
+		o1 |= 0x0010
+		o2 |= 0x0010
+	}
+	Rn := int(p.From.Reg)
+	Rt := int(p.To.Reg)
+	Rd := int(REGPC)
+	if p.Reg != 0 {
+		Rd = Rt
+		Rt = int(p.Reg)
+	}
+	out[0] = uint16(o1 | Rn&15)
+	out[1] = uint16(o2 | Rt&15<<12 | Rd&15)
+	return 4
+}
