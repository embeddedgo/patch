+// 1101 cccc iiii iiii
+func _Bcond__i8_1(c *Ctx, p *obj.Prog, out []uint16) int {
+	v := int(p.Pcond.Pc-p.Pc-pcoff) >> 1
+	out[0] = uint16(0xD000 | obcond(p.As)<<8 | v&0xFF)
+	return 2
+}
