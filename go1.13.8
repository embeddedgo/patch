+// xxxx xuuu uunn nttt
+func _MOVW__u5_2_Rn__Rt(c *Ctx, p *obj.Prog, out []uint16) int {
+	var o1 int
+	var shift uint
+	mem, r := &p.From, &p.To
+	if r.Type == obj.TYPE_REG {
+		switch p.As {
+		case AMOVW: // LDR
+			o1 = 0x6800
+			shift = 2
+		case AMOVBU: // LDRB
+			o1 = 0x7800
+		default: // AMOVHU (LDRH)
+			o1 = 0x8800
+			shift = 1
+		}
+	} else {
+		mem, r = r, mem
+		switch p.As {
+		case AMOVW: // STR
+			o1 = 0x6000
+			shift = 2
+		case AMOVB, AMOVBU: // STRB
+			o1 = 0x7000
+		default: // AMOVH, AMOVHU (STRH)
+			o1 = 0x8000
+			shift = 1
+		}
+	}
+	Rn := int(mem.Reg)
+	u5 := int(c.offset(mem)) >> shift
+	Rt := int(r.Reg)
+	out[0] = uint16(o1 | u5<<6 | Rn&7<<3 | Rt&7)
+	return 2
+}
