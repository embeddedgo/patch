+TEXT SysTick_Handler(SB),NOSPLIT|NOFRAME,$0-0
+	// set PendSV bit first to avoid DSB but ensure exception tail-chaining
+	MOVW  $ICSR_ADDR, R0
+	MOVW  $ICSR_PENDSVSET, R1
+	MOVW  R1, (R0)
