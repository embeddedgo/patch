+// 0100 000x xxmm mddd (xxx => vv)
+func _MOVW__Rdn_v_Rm__Rdn(c *Ctx, p *obj.Prog, out []uint16) int {
+	Rdn, typ, Rm := shiftr(int(p.From.Offset))
+	var o1 int
+	switch typ {
+	case 0: // <<
+		o1 = 0x4080
+	case 1: // >>
+		o1 = 0x40C0
+	case 2: // ->
+		o1 = 0x4100
+	default: // @>
+		o1 = 0x41C0
+	}
+	out[0] = uint16(o1 | Rm<<3 | Rdn)
+	return 2
+}
