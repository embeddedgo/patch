+// 1011 0010 xxmm mddd
+func _MOVH__Rm__Rd(c *Ctx, p *obj.Prog, out []uint16) int {
+	o1 := 0xB200 // AMOVH
+	switch p.As {
+	case AMOVB:
+		o1 |= 0x0040
+	case AMOVHU:
+		o1 |= 0x0080
+	case AMOVBU:
+		o1 |= 0x00C0
+	}
