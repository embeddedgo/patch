+TEXT ·Cas(SB),NOSPLIT|NOFRAME,$0
+	MOVW  ptr+0(FP), R1
+	MOVW  old+4(FP), R2
+	MOVW  new+8(FP), R3
+casl:
+	LDREX  (R1), R0
+	CMP    R0, R2
+	BNE    end
+	DMB    MB_ISHST
+	STREX  R3, (R1), R0
+	CMP    $0, R0
+	BNE    casl
+end:
+	MOVW.NE    $0, R0
+	MOVW.P.EQ  $1, R0
+	DMB.EQ     MB_ISH
+	MOVB       R0, ret+12(FP)
+	RET
+
+TEXT ·Xchg(SB),NOSPLIT|NOFRAME,$0-12
+	MOVW  addr+0(FP), R1
+	MOVW  v+4(FP), R2
+loop:
+	LDREX  (R1), R0
+	DMB    MB_ISHST
+	STREX  R2, (R1), R3
+	CMP    $0, R3
+	BNE    loop
+	DMB    MB_ISH
+	MOVW   R0, ret+8(FP)
+	RET
+
+TEXT ·Xadd(SB),NOSPLIT|NOFRAME,$0-12
+	MOVW  val+0(FP), R1
+	MOVW  delta+4(FP), R2
+loop:
