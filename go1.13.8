+//1110 1000 1101 nnnn  1111 0000 000h mmmm
+func _TBB__Rm__Rn(c *Ctx, p *obj.Prog, out []uint16) int {
+	o1, o2 := 0xE8D0, 0xF000
+	if p.As == ATBH {
+		o1 |= 0x0010
+	}
+	Rm := int(p.From.Reg)
+	Rn := int(p.Reg)
+	out[0] = uint16(o1 | Rn&15)
+	out[1] = uint16(o2 | Rm&15)
+	return 4
